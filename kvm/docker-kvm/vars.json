[
  {
  "vm_disk_bus":"ide/scsi/sata/virtio, default virtio(cdrom default sata), cdrom not support virtio",
  "vm_disk_type":"file vmdisk type(qcow2/raw), default raw",
  "vm_net_model":"virtio/rtl8139/e1000/..., default virtio",
  "vm_cpu":"host-model/host-passthrough/kvm64/IvyBridge..., default:host-model(x86), host-passthrough(arm)",
  "vm_desc":"vm describe",
  "vm_interface":"vm net interface(cloud-init), default eth0",
  "vm_ipaddr":"vm ipaddress(cloud-init)",
  "vm_gateway":"vm gateway address",
  "vm_meta_enum":"Cloud-init mode NOCLOUD/ISO,default ISO(nodef)",
  "vm_machine":"vm machine type(q35..), x86 default pc, arm default virt",
  "vm_name":"vm hostname and vm name",
  "vm_ram_mb":"vm memory(MB)",
  "vm_ram_mb_max":"vm max memory(MB)",
  "vm_user":"nonroot user (cloud-init), default clouduser",
  "vm_password":"vm_user password, default tsd@2023",
  "vm_sshkey":"sshkey for user vm_user login(cloud-init)",
  "vm_timezone":"vm timezone(cloud-init), default Asia/Shanghai",
  "vm_locale":"vm locale(cloud-init), default zh_CN.UTF-8",
  "vm_uefi":"uefi file, x86 default bios,(/usr/share/qemu/OVMF.fd)/aarch64 default(/usr/share/AAVMF/AAVMF_CODE.fd)",
  "vm_uefi_secure":"vm uefi secure(yes/no), default no",
  "vm_video":"vga/qxl/virtio/cirrus, default vga",
  "vm_sound":"es1370/sb16/ac97/ich6, default ac97",
  "vm_rng":"'no' mean no rng virtio random device, default nodefine",
  "vm_graph":"spice/vnc, default console mode no graph",
  "vm_vcpus":"vm numof cpus",
  "vm_vcpus_max":"vm max numof cpus"
  }
]
