    db.KVMDevice(kvmhost='asus',name='local-disk',devtype='disk',tpl='disk.file',action='asus-disk.file',desc='文件系统DISK'),
    db.KVMDevice(kvmhost='asus',name='net-br-ext',devtype='net',tpl='net.br-ext',action='',desc='br-ext网桥'),
